<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="m6r">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import m6r urdf file -->
    <xacro:include filename="$(find m6r_description)/urdf/m6r_0110.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="m6r.ros2_control.xacro" />


    <xacro:m6r_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
